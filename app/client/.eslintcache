imary=\"labels\"\n              onClick={() => setMenuDrawer(false)}\n            />\n\n            <ListItemLink\n              to=\"/genres\"\n              primary=\"genres\"\n              onClick={() => setMenuDrawer(false)}\n            />\n\n            <ListItemLink\n              to=\"/news\"\n              primary=\"news\"\n              onClick={() => setMenuDrawer(false)}\n            />\n\n            <Divider className={classes.divider} classes={classes.light} />\n            <ListItem>\n              <ListItemText inset={true}>catalog music 2021</ListItemText>\n            </ListItem>\n          </List>\n        </Fade>\n      </Drawer>\n    </React.Fragment>\n  );\n}\n","/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/Navbar/SearchField.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/SearchResults/SearchResults.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/Navbar/ToolBarMenuDrawer.js",[],{"ruleId":"237","replacedBy":"238"},{"ruleId":"239","replacedBy":"240"},{"ruleId":"241","severity":1,"message":"242","line":30,"column":11,"nodeType":"243","messageId":"244","endLine":30,"endColumn":15},{"ruleId":"241","severity":1,"message":"245","line":30,"column":17,"nodeType":"243","messageId":"244","endLine":30,"endColumn":23},{"ruleId":"237","replacedBy":"246"},{"ruleId":"239","replacedBy":"247"},{"ruleId":"241","severity":1,"message":"248","line":1,"column":29,"nodeType":"243","messageId":"244","endLine":1,"endColumn":38},{"ruleId":"241","severity":1,"message":"249","line":9,"column":16,"nodeType":"243","messageId":"244","endLine":9,"endColumn":19},{"ruleId":"241","severity":1,"message":"250","line":9,"column":21,"nodeType":"243","messageId":"244","endLine":9,"endColumn":28},{"ruleId":"241","severity":1,"message":"251","line":1,"column":10,"nodeType":"243","messageId":"244","endLine":1,"endColumn":14},{"ruleId":"241","severity":1,"message":"252","line":1,"column":16,"nodeType":"2