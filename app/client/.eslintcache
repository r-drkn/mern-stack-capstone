lobal();\n  const auth = useAuth();\n  const { menuDrawer, setMenuDrawer } = globe;\n\n  const ListItemLink = function (props) {\n    const { onClick, primary, to } = props;\n\n    const CustomLink = React.useMemo(\n      () =>\n        React.forwardRef((linkProps, ref) => (\n          <Link ref={ref} to={to} {...linkProps} />\n        )),\n      [to]\n    );\n\n    return (\n      <li>\n        <ListItem\n          className={classes.listItems}\n          button\n          component={CustomLink}\n          onClick={onClick}\n        >\n          <ListItemText\n            classes={{ primary: classes.listItemText }}\n            inset={true}\n            primary={primary}\n          />\n        </ListItem>\n      </li>\n    );\n  };\n\n  return (\n    <React.Fragment>\n      <IconButton\n        aria-label=\"menu\"\n        edge=\"start\"\n        onClick={() => {\n          setMenuDrawer(!menuDrawer);\n        }}\n      >\n        <MenuIcon className={classes.menuIcon} />\n      </IconButton>\n\n      <Drawer\n        className={classes.root}\n        variant=\"temporary\"\n        anchor=\"left\"\n        open={menuDrawer}\n        classes={{ paper: classes.paper }}\n      >\n        <List style={{ padding: \"0px\" }}>\n          <ListItem className={classes.toolBarContainer} key=\"toolBarContainer\">\n            <ToolBarMenuDrawer />\n          </ListItem>\n          <ListItem\n            className={classes.searchListItem}\n            alignItems=\"center\"\n            key=\"searchListItem\"\n          >\n            <SearchField />\n          </ListItem>\n          {auth.authState ? (\n            <ListItemLink\n              to=\"/account\"\n              primary=\"my account\"\n              onClick={() => setMenuDrawer(false)}\n            />\n          ) : (\n            <ListItemLink\n              to=\"\"\n              primary=\"log in\"\n              onClick={() => globe.setLoginModalState(true)}\n            />\n          )}\n\n          <ListItemLink\n            to=\"/new\"\n            primary=\"new vinyl\"\n            onClick={() => setMenuDrawer(false)}\n          />\n\n          <ListItemLink\n            to=\"/genres\"\n            primary=\"genres\"\n            onClick={() => setMenuDrawer(false)}\n          />\n\n          <ListItemLink\n            to=\"/news\"\n            primary=\"news\"\n            onClick={() => setMenuDrawer(false)}\n          />\n\n          <ListItemLink\n            to=\"/staffpicks\"\n            primary=\"staff picks\"\n            onClick={() => setMenuDrawer(false)}\n          />\n\n          <ListItemLink\n            to=\"/contact\"\n            primary=\"contact\"\n            onClick={() => setMenuDrawer(false)}\n          />\n\n          <Divider className={classes.divider} classes={classes.light} />\n          <ListItem>\n            <ListItemText inset={true}>catalog music 2020</ListItemText>\n          </ListItem>\n        </List>\n      </Drawer>\n    </React.Fragment>\n  );\n}\n","/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/RecordModal/RecordModalStyles.js",["272"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/index.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/util/fetch.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/MainContainer/MainContainer.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Home/Home.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Genres/Genres.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/context/CartContext.js",["273"],"import React, { useContext, useEffect, useReducer } from \"react\";\nimport cartReducer from \"../context/reducers/cartReducer\";\n\nconst GlobalCartContext = React.createContext();\n\nexport const useCart = () => {\n  return useContext(GlobalCartContext);\n};\n\nconst CartContext = ({ children }) => {\n  let localStorageCart = JSON.parse(localStorage.get